@namespace(uri="extractperson", prefix="")
package extractperson;

class Newsgroup {
   val Article[*] articles;
}

class Article {
   attr String articleId;
   val Person sender;
   attr String subject;
   attr String[*] newsgroups;
   attr String inReplyTo;
   attr String date;
}

class Person {
   attr String name;
   attr String email;  
}